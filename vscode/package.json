{
  "$schema": "package.schema.json",
  "name": "driver-ai",
  "private": true,
  "displayName": "Driver: AI Chat Assistant",
  "version": "0.1.0",
  "publisher": "driver",
  "license": "UNLICENSED",
  "icon": "resources/driver.png",
  "description": "Driver’s AI chat bot.",
  "scripts": {
    "build:root": "pnpm -C .. run -s build",
    "postinstall": "pnpm download-wasm && pnpm download-fonts && pnpm copy-win-ca-roots",
    "dev": "pnpm run -s dev:desktop",
    "dev:insiders": "pnpm run -s dev:desktop:insiders",
    "start:dev:desktop": "ts-node scripts/run-with-extension-development-path.ts code --disable-extension=driver-ai.driver-chat --disable-extension=github.copilot --inspect-extensions=9333 --new-window . --goto ./src/completions/inline-completion-item-provider.ts:16:5",
    "dev:desktop": "pnpm run -s build:dev:desktop && pnpm run start:dev:desktop",
    "dev:desktop:insiders": "pnpm run -s build:dev:desktop && ts-node scripts/run-with-extension-development-path.ts code-insiders --disable-extension=driver-ai.driver-chat --disable-extension=github.copilot --inspect-extensions=9333 --new-window . --goto ./src/completions/inline-completion-item-provider.ts:16:5",
    "dev:web": "pnpm run -s build:dev:web && pnpm run -s _dev:vscode-test-web --browserType none",
    "watch:dev:web": "concurrently \"pnpm run -s watch:build:dev:web\" \"pnpm run -s _dev:vscode-test-web --browserType none\"",
    "_dev:vscode-test-web": "vscode-test-web --extensionDevelopmentPath=. ${WORKSPACE-test/fixtures/workspace}",
    "build": "pnpm run -s check:build && tsc --build && pnpm run -s _build:esbuild:desktop && pnpm run -s _build:esbuild:web && pnpm run -s _build:webviews --mode production && pnpm run measure-bundle-size",
    "check:build": "pnpm run -s check:typehacks",
    "check:typehacks": "ts-node-transpile-only ./scripts/enable-typehacks.ts && tsc -p tsconfig.typehacks.json",
    "_build:desktop": "pnpm run -s _build:esbuild:desktop && pnpm run -s _build:webviews --mode production",
    "_build:web": "pnpm run -s _build:esbuild:web && pnpm run -s _build:webviews --mode production",
    "build:dev:desktop": "concurrently \"pnpm run -s _build:esbuild:desktop\" \"pnpm run -s _build:webviews --mode development\"",
    "build:dev:web": "concurrently \"pnpm run -s _build:esbuild:web\" \"pnpm run -s _build:webviews --mode development\"",
    "build:prod:webviews": "pnpm run -s _build:webviews --mode production",
    "watch:build:dev:web": "concurrently \"pnpm run -s _build:esbuild:web --watch\" \"pnpm run -s _build:webviews --mode development --watch\"",
    "watch:build:dev:desktop": "concurrently \"pnpm run -s _build:esbuild:desktop --watch\" \"pnpm run -s _build:webviews --mode development --watch\"",
    "_build:esbuild:desktop": "pnpm download-wasm && pnpm download-fonts && pnpm run -s _build:esbuild:uninstall && pnpm run -s _build:esbuild:node",
    "_build:esbuild:node": "esbuild ./src/extension.node.ts --bundle --outfile=dist/extension.node.js --loader:.node=copy --external:vscode --external:typescript --alias:@sourcegraph/cody-shared=@sourcegraph/cody-shared/src/index --alias:@sourcegraph/cody-shared/src=@sourcegraph/cody-shared/src --alias:lexical=./build/lexical-package-fix --format=cjs --platform=node --sourcemap --target=es2022",
    "_build:esbuild:web": "esbuild ./src/extension.web.ts --platform=browser --bundle --outfile=dist/extension.web.js --alias:@sourcegraph/cody-shared=@sourcegraph/cody-shared/src/index --alias:@sourcegraph/cody-shared/src=@sourcegraph/cody-shared/src --alias:path=path-browserify --external:typescript --alias:node:path=path-browserify --alias:node:os=os-browserify --alias:os=os-browserify --external:vscode  --external:node:child_process  --external:node:util --external:node:fs --external:node:fs/promises --external:node:process --define:process='{\"env\":{}}' --define:window=self --format=cjs --sourcemap",
    "_build:esbuild:uninstall": "node ./uninstall/esbuild.mjs",
    "_build:webviews": "vite -c webviews/vite.config.mts build",
    "_build:vsix_for_test": "vsce package --no-dependencies --out dist/driver-ai.e2e.vsix",
    "release": "ts-node-transpile-only ./scripts/release.ts",
    "download-wasm": "ts-node-transpile-only ./scripts/download-wasm-modules.ts",
    "download-fonts": "ts-node-transpile-only ./scripts/download-fonts.ts",
    "copy-win-ca-roots": "ts-node-transpile-only ./scripts/copy-win-ca-roots.ts",
    "release:dry-run": "pnpm run download-wasm && pnpm run download-fonts && cross-env DRIVER_RELEASE_DRY_RUN=1 ts-node ./scripts/release.ts",
    "storybook": "storybook dev -p 6007 --no-open --no-version-updates",
    "test:e2e": "playwright install && tsc --build && node dist/tsc/test/e2e/install-deps.js && pnpm run -s _build:vsix_for_test && pnpm run -s build:dev:desktop && pnpm run -s test:e2e:run",
    "test:e2e:run": "playwright test",
    "test:e2e2": "pnpm -s test:e2e2:deps && pnpm -s build:root && pnpm -s build:dev:desktop && pnpm -s test:e2e2:run",
    "test:e2e2:run": "playwright test -c playwright.v2.config.ts",
    "test:e2e2:deps": "playwright install chromium --with-deps",
    "test:integration": "tsc --build ./test/integration && pnpm run -s build:dev:desktop && node --inspect -r ts-node/register dist/tsc/test/integration/main.js",
    "test:unit": "vitest",
    "bench": "vitest bench",
    "vscode:prepublish": "pnpm -s run build",
    "vscode:uninstall": "node ./dist/post-uninstall.js",
    "test:unit:tree-sitter-queries": "vitest ./src/tree-sitter/query-tests/*.test.ts",
    "release-notes": "ts-node-transpile-only ./scripts/release-notes.ts",
    "measure-bundle-size": "cross-env LOG_BUNDLE_SIZE=true ts-node-transpile-only ./scripts/measure-bundle-size.ts"
  },
  "categories": [
    "AI",
    "Chat",
    "Programming Languages",
    "Machine Learning",
    "Snippets",
    "Education"
  ],
  "keywords": [
    "driver",
    "chop",
    "autocomplete",
    "chat",
    "chatbot",
    "code analysis",
    "code generation",
    "documentation",
    "refactor",
    "test",
    "generate",
    "openai",
    "anthropic",
    "gemini",
    "gpt-4",
    "claude",
    "intellisense",
    "openai",
    "ai",
    "assistant",
    "llm",
    "bot",
    "model",
    "typescript",
    "javascript",
    "python",
    "golang",
    "rust",
    "go",
    "html",
    "css",
    "java",
    "php",
    "swift",
    "kotlin",
    "ollama"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcegraph/cody",
    "directory": "vscode"
  },
  "bugs": {
    "url": "https://github.com/sourcegraph/cody/issues"
  },
  "homepage": "https://www.driver.ai/",
  "badges": [
    {
      "url": "https://img.shields.io/discord/969688426372825169?color=5765F2",
      "href": "https://srcgr.ph/discord",
      "description": "Discord"
    }
  ],
  "engines": {
    "vscode": "^1.79.0",
    "node": ">=20.0.0"
  },
  "main": "./dist/extension.node.js",
  "browser": "./dist/extension.web.js",
  "activationEvents": [
    "onLanguage",
    "onStartupFinished",
    "onWebviewPanel:driver-ai.chat"
  ],
  "contributes": {
    "colors": [
      {
        "id": "driver.fixup.conflictBackground",
        "description": "The background of text Driver will edit where there is a specific conflict with your changes.",
        "defaults": {
          "light": "mergeEditor.conflictingLines.background",
          "dark": "mergeEditor.conflictingLines.background"
        }
      },
      {
        "id": "driver.fixup.conflictBorder",
        "description": "The border of text Driver will edit, if there is a conflict with your changes.",
        "defaults": {
          "light": "mergeEditor.conflict.unhandledFocused.border",
          "dark": "mergeEditor.conflict.unhandledFocused.border"
        }
      },
      {
        "id": "driver.fixup.conflictedBackground",
        "description": "The background of text Driver will edit, if there is a conflict with your changes.",
        "defaults": {
          "light": "#ffffff00",
          "dark": "#00000000"
        }
      },
      {
        "id": "driver.fixup.conflictedBorder",
        "description": "The border of text Driver will edit, if there is a conflict with your changes.",
        "defaults": {
          "light": "mergeEditor.conflict.unhandledUnfocused.border",
          "dark": "mergeEditor.conflict.unhandledUnfocused.border"
        }
      },
      {
        "id": "driver.fixup.incomingBackground",
        "description": "The background of text Driver will edit.",
        "defaults": {
          "light": "merge.incomingContentBackground",
          "dark": "merge.incomingContentBackground"
        }
      },
      {
        "id": "driver.fixup.incomingBorder",
        "description": "The border around text Driver will edit.",
        "defaults": {
          "light": "#436EB1",
          "dark": "#436EB1"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "driver-ai",
          "title": "Driver",
          "icon": "resources/driver.svg"
        }
      ]
    },
    "views": {
      "driver-ai": [
        {
          "type": "webview",
          "id": "driver-ai.chat",
          "name": "Chat"
        }
      ]
    },
    "commands": [
      {
        "command": "driver-ai.viewDriverDocs",
        "category": "Driver",
        "title": "View Driver Docs",
        "enablement": "driver-ai.activated"
      },
      {
        "command": "driver-ai.viewDriverDocsOnline",
        "category": "Driver",
        "title": "View Driver Docs on the Web",
        "enablement": "driver-ai.activated"
      },
      {
        "command": "driver-ai.auth.signout",
        "category": "Driver",
        "title": "Sign Out",
        "icon": "$(sign-out)",
        "enablement": "driver-ai.activated"
      },
      {
        "command": "driver-ai.auth.signin",
        "category": "Driver",
        "title": "Switch Account…"
      },
      {
        "command": "driver-ai.settings.extension.chat",
        "category": "Driver",
        "title": "Chat Settings",
        "group": "Driver",
        "icon": "$(gear)"
      },
      {
        "command": "driver-ai.chat.signIn",
        "category": "Driver",
        "group": "Driver",
        "title": "Sign In",
        "enablement": "!driver-ai.activated"
      },
      {
        "command": "driver-ai.chat.new",
        "category": "Driver",
        "title": "New Chat",
        "enablement": "driver-ai.activated",
        "group": "Driver",
        "icon": "$(new-comment-icon)"
      },
      {
        "command": "driver-ai.chat.newEditorPanel",
        "category": "Driver",
        "title": "New Chat in Editor",
        "enablement": "driver-ai.activated",
        "group": "Driver",
        "icon": "$(new-comment-icon)"
      },
      {
        "command": "driver-ai.chat.newPanel",
        "category": "Driver",
        "group": "Driver",
        "title": "New Chat in Sidebar",
        "enablement": "driver-ai.activated",
        "icon": "$(new-comment-icon)"
      },
      {
        "command": "driver-ai.chat.moveFromEditor",
        "category": "Driver",
        "title": "Move Chat from Editor to Main Driver Panel",
        "when": "driver-ai.activated && view == driver-ai.chat",
        "enablement": "driver-ai.activated",
        "group": "Driver",
        "icon": "$(layout-sidebar-left)"
      },
      {
        "command": "driver-ai.chat.view.popOut",
        "category": "Driver",
        "title": "Pop out",
        "enablement": "driver-ai.activated",
        "group": "Driver",
        "icon": "$(link-external)"
      },
      {
        "command": "driver-ai.chat.history.clear",
        "category": "Driver",
        "title": "Delete All Chats",
        "group": "Driver",
        "icon": "$(trash)",
        "enablement": "driver-ai.activated"
      },
      {
        "command": "driver-ai.chat.history.delete",
        "category": "Driver",
        "title": "Delete Chat",
        "group": "Driver",
        "icon": "$(trash)",
        "enablement": "driver-ai.activated"
      },
      {
        "command": "driver-ai.chat.history.export",
        "category": "Driver",
        "title": "Export Chats as JSON",
        "group": "Driver",
        "icon": "$(arrow-circle-down)",
        "enablement": "driver-ai.activated"
      },
      {
        "command": "driver-ai.chat.history.panel",
        "category": "Driver",
        "title": "Chat History",
        "group": "Driver",
        "icon": "$(list-unordered)",
        "enablement": "driver-ai.activated"
      },
      {
        "command": "driver-ai.search.index-update",
        "category": "Driver",
        "group": "Driver",
        "title": "Update search index for current workspace folder",
        "icon": "$(refresh)",
        "enablement": "driver-ai.activated"
      },
      {
        "command": "driver-ai.mention.selection",
        "category": "Driver",
        "group": "Chat",
        "title": "Add Selection to Driver Chat",
        "icon": "$(mention)",
        "enablement": "driver-ai.activated && editorHasSelection"
      },
      {
        "command": "driver-ai.mention.file",
        "category": "Driver",
        "group": "Chat",
        "title": "Add File to Driver Chat",
        "icon": "$(mention)",
        "enablement": "driver-ai.activated && resourceSet"
      },
      {
        "command": "driver-ai.debug.export.logs",
        "category": "Driver Debug",
        "group": "Debug",
        "title": "Export Logs…"
      }
    ],
    "keybindings": [
      {
        "command": "driver-ai.chat.signin",
        "key": "alt+l",
        "when": "!driver-ai.activated"
      },
      {
        "command": "driver-ai.chat.signin",
        "key": "alt+k",
        "when": "!driver-ai.activated"
      },
      {
        "command": "driver-ai.chat.toggle",
        "key": "alt+l",
        "when": "driver-ai.activated"
      },
      {
        "command": "driver-ai.mention.selection",
        "key": "alt+l",
        "when": "driver-ai.activated && editorTextFocus && editorHasSelection"
      },
      {
        "command": "driver-ai.chat.new",
        "key": "shift+alt+/",
        "when": "driver-ai.activated && !editorTextFocus"
      }
    ],
    "submenus": [
      {
        "label": "Driver",
        "id": "driver-ai.submenu"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "driver-ai.chat.signIn",
          "when": "!driver-ai.activated"
        },
        {
          "command": "driver-ai.chat.view.popOut",
          "when": "false",
          "_comment": "Hidden because it is only a wrapper around the workspace pop out command and would place any editor tab (not just Driver chat) in a new window."
        },
        {
          "command": "driver-ai.chat.newEditorPanel",
          "when": "driver-ai.activated"
        }
      ],
      "editor/context": [
        {
          "submenu": "driver-ai.submenu",
          "group": "0_driver_ai"
        }
      ],
      "driver-ai.submenu": [
        {
          "command": "driver-ai.mention.selection",
          "group": "0_driver_ai",
          "when": "driver-ai.activated && editorHasSelection"
        },
        {
          "command": "driver-ai.mention.file",
          "group": "0_driver_ai",
          "when": "driver-ai.activated && !editorHasSelection"
        },
        {
          "command": "driver-ai.chat.newEditorPanel",
          "when": "driver-ai.activated && !editorHasSelection",
          "group": "ask"
        },
        {
          "command": "driver-ai.chat.signIn",
          "when": "!driver-ai.activated",
          "group": "other"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Driver AI",
      "properties": {
        "driver-ai.apiUrl": {
          "order": 1,
          "type": "string",
          "description": "URL to the Driver AI API instance.",
          "default": "https://api.dev.driverai.com"
        },
        "driver-ai.appUrl": {
          "order": 2,
          "type": "string",
          "description": "URL to redirect to for authentication.",
          "default": "https://app.dev.driverai.com"
        },
        "driver-ai.chat.defaultLocation": {
          "order": 6,
          "type": "string",
          "enum": [
            "sticky",
            "sidebar",
            "editor"
          ],
          "markdownDescription": "Controls where the Driver chat view opens when the user invokes the `Driver AI: New Chat` command, or the Alt+L and Alt+/ shortcuts.",
          "enumDescriptions": [
            "Opens in the last-activated view location, which is set whenever the user explicitly chooses to open chat in a given location",
            "Opens in the sidebar",
            "Opens in an editor panel"
          ],
          "default": "sticky"
        },
        "driver-ai.codeActions.enabled": {
          "order": 11,
          "title": "Driver Code Actions",
          "type": "boolean",
          "markdownDescription": "Add Driver options to Quick Fix menus for fixing, explaining, documenting, and editing code.",
          "default": true
        },
        "driver-ai.debug.verbose": {
          "order": 99,
          "type": "boolean",
          "markdownDescription": "Enables verbose debug output. Debug messages may contain more details if the invocation includes verbose information."
        },
        "driver-ai.agentic.context.experimentalOptions": {
          "type": "object",
          "properties": {
            "shell": {
              "allow": {
                "type": "array",
                "default": []
              },
              "block": {
                "type": "array",
                "default": []
              }
            }
          },
          "default": {},
          "examples": [
            {
              "shell": {
                "allow": [
                  "git",
                  "ls",
                  "find"
                ],
                "block": [
                  "history",
                  "sudo",
                  "rm"
                ]
              }
            }
          ],
          "markdownDescription": "'shell' field is used to configure shell command execution for agents (like Deep Driver) when terminal context is enabled. The 'shell' field has two properties:\n\n- 'allow': Accepts either [] to allow all commands or an array of specific command prefixes\n- 'block': An array of command prefixes to block, which combines with built-in safety blocklist\n\nBlocked commands take precedence over allowed commands. Command outputs are used as context. Remove the 'shell' field to allow all commands."
        }
      }
    }
  },
